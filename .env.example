# ===========================================
# ClaudeN8N SDK Configuration
# ===========================================
# Copy this file to .env and fill in the values

# Anthropic API
ANTHROPIC_API_KEY=sk-ant-xxx

# n8n Instance
N8N_API_URL=http://localhost:5678
N8N_API_KEY=your-n8n-api-key

# MCP Server Configuration
MCP_N8N_PATH=/path/to/n8n-mcp/build/index.js

# Session Configuration
SESSION_STORAGE_PATH=./sessions
MAX_CONTEXT_TOKENS=200000

# Logging
LOG_LEVEL=info
DEBUG=sdk:*

# Models (optional overrides)
# ARCHITECT_MODEL=claude-sonnet-4-20250514
# RESEARCHER_MODEL=claude-sonnet-4-20250514
# BUILDER_MODEL=claude-opus-4-5-20251101
# QA_MODEL=claude-sonnet-4-20250514
# ANALYST_MODEL=claude-sonnet-4-20250514
