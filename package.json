{
  "name": "clouden8n-sdk",
  "version": "1.0.2",
  "description": "ClaudeN8N 5-Agent System migrated to Claude Agent SDK",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "analyze": "node dist/index.js analyze",
    "fix": "node dist/index.js fix",
    "dev:analyze": "tsx src/index.ts analyze",
    "dev:fix": "tsx src/index.ts fix",
    "build": "tsc",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:run": "vitest run",
    "lint": "eslint src/",
    "prepare": "npm run build",
    "interactive": "INTERACTIVE_MODE=true node dist/index.js"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.76",
    "@modelcontextprotocol/sdk": "^1.25.1",
    "dotenv": "^16.4.7",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "eslint": "^9.17.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "claude",
    "agent-sdk",
    "n8n",
    "workflow",
    "automation"
  ],
  "author": "Sergey",
  "license": "MIT"
}
